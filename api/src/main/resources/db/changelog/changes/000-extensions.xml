<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.21.xsd">

    <changeSet id="000-enable-pgcrypto" author="dariomatias">
        <sql>CREATE EXTENSION IF NOT EXISTS pgcrypto;</sql>
    </changeSet>

    <changeSet id="000-enable-postgis" author="dario">
        <sql>CREATE EXTENSION IF NOT EXISTS postgis;</sql>
    </changeSet>

</databaseChangeLog>
